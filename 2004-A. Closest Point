#include <bits/stdc++.h>
using namespace std;
using ll = long long;

int main() 
{
	int t;
	cin >> t;
	while(t--){
		int n;
		cin >> n;
		vector<int> v(n+1);
		for(int i = 1; i <= n; i++){
			cin >> v[i];
		}
		if(n == 2 && (v[2] - v[1] != 1)) cout << "YES";
		else cout << "NO";
		cout << endl;
	}
}
