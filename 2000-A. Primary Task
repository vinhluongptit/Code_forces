#include <bits/stdc++.h>
using namespace std;
using ll = long long;

int main() 
{
	int t;
	cin >> t;
	while(t--){
		string s;
		cin >> s;
		int ok = 1;
		if(s[0] == '1' && s[1] == '0'){
			
		}else ok = 0;
		if(s[2] == '0') ok = 0;
		if(s.size() == 3 && (s[2] == '1' || s[2] == '0')) ok = 0;
		if(s.size() == 2 || s.size() == 1) ok = 0;
		if(ok) cout << "YES";
		else cout << "NO";
		cout << endl;
	}
}
