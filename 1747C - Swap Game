#include <bits/stdc++.h>
using namespace std;
using ll = long long;

int main() 
{
  int t;
  cin >> t;
  while(t--){
    int n;
    cin >> n;
    vector<int> v(n);
    for(int i = 0; i < n; i++){
      cin >> v[i];
    }
    sort(v.begin()+1, v.end());
    if(v[0] > v[1]) cout << "Alice";
    else cout << "Bob";
    cout << endl;
  }
}
