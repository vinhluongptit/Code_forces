#include <bits/stdc++.h>
using namespace std;
using ll = long long;


int main() 
{
  int t;
  cin >> t;
  while(t--){
    int n;
    cin >> n;
    if(n % 2){
      cout << "NO\n";continue;
    }
    else{
      cout << "YES\n";
      while(n >= 2){
        cout << "AA";
        n -= 2;
        if(n != 0) {
          cout << "BB";
          n -= 2;
        }
    }
    cout << endl;
  }
  }
}
