#include <bits/stdc++.h>
using namespace std;
using ll = long long;


int main() 
{
  int t;
  cin >> t;
  while(t--){
    int x1, y1, x2, y2;
    cin >> x1 >> y1 >> x2 >> y2;
    int ok1;
    if(x1 > y1) ok1 = 1;
    else ok1 = 0;
    if(ok1){
      if(x2 > y2) cout << "Yes";
      else cout << "No";
    }else{
      if(x2 < y2) cout << "Yes";
      else cout << "No";
    }
    cout << endl;
  }
}
