#include <bits/stdc++.h>
using namespace std;
int main() 
{
  int t;
  cin >> t;
  while(t--){
    int n, m;
    cin >> n >> m;
    int ok;
    if(n < m) ok = 0;
    else{
      if((n-m)%2 == 1) ok = 0;
      else ok = 1;
    }
    if(ok) cout << "Yes";
    else cout << "No";
    
    cout << endl;
  }
}
