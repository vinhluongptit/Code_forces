#include <bits/stdc++.h>
using namespace std;


int main() 
{
  int t;
  cin >> t;
  while(t--){
    int n, m, k;
    cin >> n>> m >> k;
    for(int i = n-1; i >= k-1; i--){
      cout << i+1 << " ";
    }
    for(int i = m; i < k-1; i++){
      cout << i+1 << " ";
    }
    for(int i = 1; i <= m; i++){
      cout << i << " ";
    }
    cout << endl;
  }
}
