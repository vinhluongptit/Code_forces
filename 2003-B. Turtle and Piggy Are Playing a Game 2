#include <bits/stdc++.h>
using namespace std;
using ll = long long;

int main() 
{
	int t;
	cin >> t;
	while(t--){
		int n;
		cin >> n;
		vector<int> v(n);
		for(int i = 0; i < n; i++){
			cin >> v[i];
		}
		sort(v.begin(), v.end());
		if(n & 1){
			cout << v[n/2];
		}else{
			cout << v[n/2];
		}
		cout << endl;
	}
}
