#include <iostream>
using namespace std;
#include <bits/stdc++.h>
using ll = long long;
 
int main() 
{
  int t;
  cin >> t;
  while(t--){
    ll a, b, c;
    cin >> a >>b >> c;
    if(a < b && b <c) cout << "STAIR";
    else if(a<b&&b>c) cout << "PEAK";
    else cout <<"NONE";
    cout << endl;
  }
}
